{
    "name": "drupal/cmc",
    "description": "Cacheability Metadata Checker with custom coding standards.",
    "type": "drupal-module",
    "require": {
        "php": ">=8.1",
        "drupal/core": "^10"
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Drupal\\cmc\\": "src/",
            "Drupal\\cmc\\Sniffs\\": "phpcs/Sniffs/"
        }
    }
} 